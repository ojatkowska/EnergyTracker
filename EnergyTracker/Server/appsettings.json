{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=EnergyTrackerDB;Trusted_Connection=True;MultipleActiveResultSets=true",
    "HangfireConnection": "Server=(localdb)\\mssqllocaldb;Database=HangfireDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Auth0": {
    "Authority": "https://energytracker.eu.auth0.com",
    "ApiIdentifier": "https://energytracker.com/api"
  },
  "Secrets": {
    "ForecastApiKey": "api_key"
  },
  "Hangfire": {
    "KseDataDownloadJob": "10 30 * * * ?",
    "WeatherDataDownloadJob": "10 30 * * * ?",
    "ForecastDataDownloadJob": "0 30 1 * * ?"
  },
  "AllowedHosts": "*"
}
